{
  "name": "hyperapp-middleware",
  "version": "0.3.0",
  "description": "Build HOAs for Hyperapp",
  "main": "dist/middleware.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okwolf/hyperapp-middleware.git"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "test": "jest --coverage --no-cache",
    "build": "npm run bundle && npm run minify",
    "bundle": "rollup -i src/index.js -o dist/middleware.js -m -f umd -n middleware",
    "minify": "uglifyjs dist/middleware.js -o dist/middleware.js --mangle --compress warnings=false -p relative --source-map dist/middleware.js.map",
    "prepare": "npm run build",
    "format": "prettier --semi false --write '{src,test}/**/*.js'"
  },
  "babel": {
    "presets": "env"
  },
  "devDependencies": {
    "babel-preset-env": "1.6.0",
    "hyperapp": "^0.16.0",
    "jest": "^21.2.1",
    "prettier": "^1.7.2",
    "rollup": "^0.50.0",
    "uglify-js": "^2.7.5"
  },
  "peerDependencies": {
    "hyperapp": "^0.16.0"
  },
  "author": "Wolfgang Wedemeyer <wolf@okwolf.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/okwolf/hyperapp-middleware/issues"
  },
  "homepage": "https://github.com/okwolf/hyperapp-middleware"
}
